HELLO [<%= current_user.email.split("@")[0].upcase %>]<br>
BALANCE: $<%= current_user.balance %><br>
<%= link_to 'deposit', user_deposit_path %>
<%= link_to 'withdraw', user_withdraw_path %>
<br>
STOCKS: <%= @transactions.length %>
<br>
<br>
<% @transactions.each do |transaction| %>
  Symbol: <%= transaction.symbol %><br>
  Company name: <%= transaction.company_name %><br>
  Price: $<%= transaction.stock_price %><br>
  Stock: <%= transaction.stock_quantity %><br>
  <%= link_to "sell", transactions_sell_path(transaction.id) %>
  <%= link_to 'buy', transactions_buy_path(SS: transaction.symbol, SID: transaction.stock_id) %>
  <br>
  <br>
<% end %>
<br>
